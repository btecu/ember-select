{
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-transform-class-properties", { "loose": true }],
    "@embroider/addon-dev/template-colocation-plugin",
    [
      "babel-plugin-ember-template-compilation",
      {
        "targetFormat": "hbs",
        "transforms": []
