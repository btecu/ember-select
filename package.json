{
  "name": "ember-select",
  "version": "0.9.0",
  "description": "Select component",
  "keywords": [
    "ember-addon",
    "ember-select",
    "select",
    "dropdown",
    "selectize",
    "power-select"
  ],
  "license": "MIT",
  "author": "Bujorel Tecu",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/btecu/ember-select",
  "scripts": {
    "build": "ember build",
    "lint": "concurrently \"yarn:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"yarn:lint:css -- --fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "concurrently \"yarn:lint\" \"yarn:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.0",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^5.2.1",
    "@embroider/test-setup": "^4.0.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.1.2",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "~5.12.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^3.0.1",
    "ember-qunit": "^9.0.2",
    "ember-resolver": "^13.1.0",
    "ember-source": "~5.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-try": "^4.0.0",
    "ember-template-lint": "^7.2.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-qunit": "^8.1.2",
    "loader.js": "^4.7.0",
    "prettier": "^3.5.3",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-prettier": "^5.0.3",
    "webpack": "^5.99.7"
  },
  "engines": {
    "node": ">= 20"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
