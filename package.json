{
  "name": "ember-select",
  "version": "0.9.0",
  "description": "Select component",
  "keywords": [
    "ember-addon",
    "ember-select",
    "select",
    "dropdown",
    "selectize",
    "power-select"
  ],
  "license": "MIT",
  "author": "Bujorel Tecu",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/btecu/ember-select",
  "scripts": {
    "build": "ember build",
