{
  "name": "test-app",
  "version": "0.9.5",
  "description": "Select component",
  "keywords": [
    "ember-select",
    "select",
    "dropdown",
    "selectize",
    "power-select"
  ],
  "license": "MIT",
  "author": "Bj Tecu",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/btecu/ember-select",
  "scripts": {
    "build": "vite build",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input --cache",
    "lint:css:fix": "stylelint \"**/*.css\" --allow-empty-input --fix",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "vite",
    "test": "pnpm test:ember",
    "test:ember-compatibility": "ember try:one",
    "test:ember": "vite build --mode development && ember test --path dist"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@ember/optional-features": "^2.2.0",
    "@ember/string": "^4.0.0",
    "@ember/test-helpers": "^5.4.0",
    "@embroider/broccoli-side-watch": "^1.1.0",
    "@embroider/compat": "^4.0.3",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^4.2.5",
    "@embroider/test-setup": "^4.0.0",
    "@embroider/vite": "^1.3.6",
    "@eslint/js": "^9.36.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "@rollup/plugin-babel": "^6.1.0",
    "babel-plugin-ember-template-compilation": "^2.3.0",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.3.0",
    "ember-auto-import": "^2.11.2",
    "ember-cli": "^6.8.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-load-initializers": "^3.0.1",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-select": "workspace:*",
    "ember-source": "^6.8.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^7.9.3",
    "ember-try": "^4.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-qunit": "^8.2.5",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "qunit": "^2.24.2",
    "qunit-dom": "^3.5.0",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-prettier": "^5.0.3",
    "vite": "^7.2.2"
  },
  "engines": {
    "node": ">= 20"
  },
  "ember": {
    "edition": "octane"
  },
  "packageManager": "pnpm@10.18.0",
  "volta": {
    "node": "22.20.0",
    "pnpm": "10.18.0"
  },
  "private": true,
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  }
}
